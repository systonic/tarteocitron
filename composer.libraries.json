{
    "name": "systonic/tarteocitron",
    "description": "Add the tarteaucitron.js service.",
    "type": "drupal-module",
    "keywords": ["php", "drupal", "seo", "rgpd", "gdpr"],
    "homepage": "https://github.com/systonic/tarteocitron",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Sylvain Larras"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "amauric.tarteaucitron": {
            "type": "package",
            "package": {
                "name": "amauric/tarteaucitron",
                "version": "1.2.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "tarteaucitron.js"
